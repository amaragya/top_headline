<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/aktif_layanan"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@drawable/circle_pizza"
        android:gravity="right|center_vertical"
        android:paddingBottom="30dp"
        android:paddingLeft="10dp"
        android:paddingRight="35dp"
        android:paddingTop="10dp"
        android:text="LAYANAN jamaah haji"
        android:textColor="@android:color/white"
        android:textSize="10dp" />

    <ImageView
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_gravity="center_vertical|center_horizontal"
        android:background="@drawable/circle_outline" />

    <FrameLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|center_horizontal">

        <TextView
            android:id="@+id/bt_item"
            android:layout_width="75dp"
            android:layout_height="75dp"
            android:layout_gravity="center_vertical|center_horizontal"
            android:background="@drawable/circle"
            android:gravity="center"
            android:padding="10dp"
            android:text="LAYANAN 1"
            android:textColor="@android:color/white"
            android:textSize="10dp" />

    </FrameLayout>

    <FrameLayout

        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

        <com.jh.circularlist.CircularListView
            android:id="@+id/my_circular_list"
            android:layout_width="350dp"
            android:layout_height="350dp"
            android:layout_gravity="center_vertical|center_horizontal" />
    </FrameLayout>


</FrameLayout>
